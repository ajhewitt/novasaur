ADDR 0x40             # PCL = 255, iden PCH, inc PCH
LD HL, $IDEN$2COM
LDZ Y, $PCH
FNH DZ, Y             # Y = PCH
FNFL M, HL            # HL = -[PC]
#41
LD Y, $AREG
