FNFH DZ, NA           # A = low reg
#71
LDZ Y, $SPH
FNH DZ, Y             # Y = SPH
FNFH A, NM            # A -> [SP]
LD HL, $FETCH
#80
LD Y, $VMS            # set Y = $VMS on exit
VMPHL DZ, PGA         # jump to next VMC
#86

ADDR 0xC0             # else, iden SPH, iden SPH
NOP
NOP
NOP
NOP
LD HL, $IDEN$SRC2RPH
LD Y, $INST
#41
FNEL DZ, Y            # Y = zero page addr of high reg
FNFH DZ, NA           # A = high reg
LDZ Y, $SPH
FNH DZ, Y             # Y = SPH
#51
FNFH A, NM            # A -> [SP]
LD HL, $DEC$NULL
